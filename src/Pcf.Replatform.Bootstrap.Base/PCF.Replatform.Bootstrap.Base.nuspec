  <metadata>
    <id>PivotalServices.CloudFoundry.Replatform.Bootstrap.Base</id>
    <version>$Version$</version>
    <title>PivotalServices.CloudFoundry.Replatform.Bootstrap.Base</title>
    <authors>ajaganathan</authors>
