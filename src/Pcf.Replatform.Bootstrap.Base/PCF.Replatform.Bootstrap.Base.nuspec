<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>PivotalServices.AspNet.Replatform.Cf.Base</id>
    <version>$Version$</version>
    <title>PivotalServices.AspNet.Replatform.Cf.Base</title>
    <authors>ajaganathan,Pivotal</authors>
    <owners>ajaganathan</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <description>Package helps to bootstrap a full framework application to PCF - supports DI (native, autofac and unity). Additional extension packages (PivotalServices.CloudFoundry.Replatform.Bootstrap.*) are available based on specific needs like persisting session to redis, cloud native logging, configuration externalization, spring boot actuators, etc.</description>
    <copyright>$PivotalServicesBootstrapCopyright$</copyright>
    <repository type="git" url="https://github.com/alfusinigoj/pivotal_cloudfoundry_replatform_bootstrap.git"/>
    <tags>ASP.NET; CloudFoundry; Replatform</tags>
    <dependencies>
      <group targetFramework="$TargetFrameworkGeneral$">
        <dependency id="Microsoft.AspNet.Mvc" version ="$MicrosoftAspNetVersion$"/>
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version ="$MicrosoftAspNetVersion$"/>
        <dependency id="Microsoft.Extensions.Hosting" version ="$MicrosoftExtensionsVersion$"/>
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version ="$MicrosoftExtensionsVersion$"/>
        <dependency id="Microsoft.Extensions.Configuration.EnvironmentVariables" version ="$MicrosoftExtensionsConfigurationEnvironmentVariableVersion$"/>
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web"  targetFramework="$TargetFrameworkGeneral$" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="readme.txt" target="" />
  </files>
</package>
