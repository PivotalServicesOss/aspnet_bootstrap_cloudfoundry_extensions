<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>PivotalServices.AspNet.Replatform.Cf.Redis.Session</id>
    <version>$Version$</version>
    <title>PivotalServices.AspNet.Replatform.Cf.Redis.Session</title>
    <authors>ajaganathan,Pivotal</authors>
    <owners>ajaganathan</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <description>Package helps to bootstrap a full framework application to PCF with persisting session to Redis</description>
    <copyright>$PivotalServicesBootstrapCopyright$</copyright>
    <repository type="git" url="https://github.com/alfusinigoj/pivotal_cloudfoundry_replatform_bootstrap.git"/>
    <tags>ASP.NET; CloudFoundry; Replatform; Redis; Steeltoe; Session</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework="$TargetFrameworkGeneral$"/>
      <frameworkAssembly assemblyName="System.Configuration" targetFramework="$TargetFrameworkGeneral$"/>
    </frameworkAssemblies>
    <dependencies>
      <group targetFramework="$TargetFrameworkGeneral$">
        <dependency id="Microsoft.Web.RedisSessionStateProvider" version ="$MicrosoftWebRedisVersion$"/>
        <dependency id="Steeltoe.CloudFoundry.ConnectorCore" version ="$SteeltoeVersion$"/>
        <dependency id="PivotalServices.AspNet.Replatform.Cf.Base" version ="$PivotalServicesBootstrapPackageVersion$"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Web.config.install.xdt" target="Content" />
    <file src="Web.config.uninstall.xdt" target="Content" />
    <file src="readme.txt" target="" />
  </files>
</package>
