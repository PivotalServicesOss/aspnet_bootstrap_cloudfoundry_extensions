<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\versions.props" />
  <PropertyGroup>
    <TargetFramework>net471</TargetFramework>
    <RootNamespace>PivotalServices.CloudFoundry.Replatform.Bootstrap.Redis.Sesion</RootNamespace>
    <AssemblyName>PivotalServices.CloudFoundry.Replatform.Bootstrap.Redis.Sesion</AssemblyName>
  </PropertyGroup>

  <ItemGroup Condition="'$(Configuration)' == 'Debug'">
    <ProjectReference Include="..\Pcf.Replatform.Bootstrap.Base\Pcf.Replatform.Bootstrap.Base.csproj" />
  </ItemGroup>
  <ItemGroup Condition="'$(Configuration)' == 'Release'">
    <PackageReference Include="PivotalServices.CloudFoundry.Replatform.Bootstrap.Base" Version="$(PivotalServicesBootstrapBaseVersion)" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.RedisSessionStateProvider">
      <Version>$(MicrosoftWebRedisVersion)</Version>
    </PackageReference>
    <PackageReference Include="Steeltoe.CloudFoundry.ConnectorCore">
      <Version>$(SteeltoeVersion)</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Configuration" />
    <Reference Include="System.Web" />
  </ItemGroup>
  
</Project>
